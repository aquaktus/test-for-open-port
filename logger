  console.log('hello world!');
  res.sendfile('/home/centos/Crayonn/crayonn/images/57389b004212b1f05b164c95/fullSize.jpg');
});

app.listen(3000, '0.0.0.0', function() {
  console.log('I\'m Listening...');
})
